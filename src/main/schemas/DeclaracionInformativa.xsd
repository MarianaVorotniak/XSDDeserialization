<?xml version="1.0" encoding="ISO-8859-1"?>
<schema elementFormDefault="qualified" targetNamespace="https://www2.agenciatributaria.gob.es/static_files/common/internet/dep/aplicaciones/es/aeat/ddii/enol/ws/DeclaracionInformativa.xsd" xmlns:ddii="https://www2.agenciatributaria.gob.es/static_files/common/internet/dep/aplicaciones/es/aeat/ddii/enol/ws/DeclaracionInformativa.xsd" xmlns="http://www.w3.org/2001/XMLSchema">
    <!-- Suministro genérico -->
    <complexType name="DeclaracionInformativa">
        <annotation>
            <documentation xml:lang="es"> Ddii - Suministro Inmediato de Declaraciones Informativas. </documentation>
        </annotation>
        <sequence>
            <element name="Cabecera" type="ddii:CabeceraDI"/>
            <!--Current configuration of the parser doesn't allow a maxOccurs attribute value to be set greater than the value 5 000.-->
            <element name="Declarado" type="ddii:DeclaradoType" maxOccurs="10000"/>
        </sequence>
    </complexType>
    <!-- Cabecera del suministro de información -->
    <complexType name="CabeceraDI">
        <annotation>
            <documentation xml:lang="es"> Datos de contexto de un suministro </documentation>
        </annotation>
        <sequence>
            <element name="TipoComunicacion" type="ddii:ClaveTipoComunicacionType"/>
            <element name="Modelo" type="ddii:ModeloType"/>
            <element name="Periodo">
                <complexType>
                    <annotation>
                        <documentation xml:lang="es"> Período al que corresponden los apuntes. todos los apuntes deben corresponder al mismo período impositivo </documentation>
                    </annotation>
                    <sequence>
                        <element name="Ejercicio" type="ddii:YearType"/>
                        <element name="Periodo" type="ddii:TipoPeriodoType"/>
                    </sequence>
                </complexType>
            </element>
            <element name="IDVersionModelo" type="ddii:VersionModeloType"/>
            <element name="IDDeclarante">
                <complexType>
                    <sequence>
                        <element name="NIF" type="ddii:NIFType"/>
                        <element name="NombreRazon" type="ddii:TextMax120Type"/>
                        <element name="NIFRepresentante" type="ddii:NIFType" minOccurs="0"/>
                        <element name="PersonaContacto" minOccurs="0">
                            <complexType>
                                <sequence>
                                    <element name="Telefono" type="ddii:TextMax12Type"/>
                                    <element name="ApellidosNombre" type="ddii:TextMax120Type"/>
                                </sequence>
                            </complexType>
                        </element>
                    </sequence>
                </complexType>
            </element>
        </sequence>
    </complexType>
    <!-- Consulta -->
    <complexType name="ConsultaDI">
        <annotation>
            <documentation xml:lang="es"> Ddii - Consulta Suministro Inmediato de Declaraciones Informativas. </documentation>
        </annotation>
        <sequence>
            <element name="Cabecera" type="ddii:CabeceraConsultaDI"/>
        </sequence>
    </complexType>
    <!-- Información básica que contienen los registros del suministro de información -->
    <complexType name="CabeceraConsultaDI">
        <annotation>
            <documentation xml:lang="es"> Datos de contexto de un suministro </documentation>
        </annotation>
        <sequence>
            <element name="Modelo" type="ddii:ModeloType"/>
            <element name="Periodo">
                <complexType>
                    <annotation>
                        <documentation xml:lang="es"> Período al que corresponden los apuntes. todos los apuntes deben corresponder al mismo período impositivo </documentation>
                    </annotation>
                    <sequence>
                        <element name="Ejercicio" type="ddii:YearType"/>
                        <element name="Periodo" type="ddii:TipoPeriodoType"/>
                    </sequence>
                </complexType>
            </element>
            <element name="IDDeclarante">
                <complexType>
                    <sequence>
                        <element name="NIF" type="ddii:NIFType"/>
                        <element name="NombreRazon" type="ddii:TextMax120Type"/>
                    </sequence>
                </complexType>
            </element>
        </sequence>
    </complexType>
    <!-- Baja -->
    <complexType name="BajaDI">
        <annotation>
            <documentation xml:lang="es"> Ddii - Baja Suministro Inmediato de Declaraciones Informativas. </documentation>
        </annotation>
        <sequence>
            <element name="Cabecera" type="ddii:CabeceraConsultaDI"/>
            <element name="Declarado" type="ddii:DeclaradoType2"/>
        </sequence>
    </complexType>
    <!-- -->
    <complexType name="DeclaradoType">
        <annotation>
            <documentation xml:lang="es">Datos correspondientes al Declarado </documentation>
        </annotation>
        <sequence>
            <element name="IDRegistroDeclarado" type="ddii:TextMax20Type"/>
            <element name="IDDeclarado" maxOccurs="unbounded">
                <complexType>
                    <annotation>
                        <documentation xml:lang="es"> Datos del declarado </documentation>
                    </annotation>
                    <sequence>
                        <element name="Clave" type="ddii:ClaveType"/>
                        <element name="NombreRazon" type="ddii:TextMax120Type"/>
                        <element name="NIFRepresentante" type="ddii:NIFType" minOccurs="0"/>
                        <choice>
                            <element name="NIF" type="ddii:NIFType"/>
                            <element name="IDOtro" type="ddii:IDOtroType"/>
                        </choice>
                    </sequence>
                </complexType>
            </element>
            <element name="Detalle">
                <complexType>
                    <sequence>
                        <element name="IDCesionario" maxOccurs="unbounded">
                            <complexType>
                                <annotation>
                                    <documentation xml:lang="es"> Datos del cesionario </documentation>
                                </annotation>
                                <sequence>
                                    <element name="NombreRazon" type="ddii:TextMax120Type"/>
                                    <element name="NIFRepresentante" type="ddii:NIFType" minOccurs="0"/>
                                    <choice>
                                        <element name="NIF" type="ddii:NIFType"/>
                                        <element name="IDOtro" type="ddii:IDOtroType"/>
                                    </choice>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="IDInmueble">
                            <complexType>
                                <annotation>
                                    <documentation xml:lang="es"> Datos del inmueble </documentation>
                                </annotation>
                                <sequence>
                                    <element name="SituacionInmueble" type="ddii:SituacionInmuebleType"/>
                                    <element name="DatosInmueble">
                                        <complexType>
                                            <sequence>
                                                <element name="ReferenciaCatastral" type="ddii:ReferenciaCatastralType" minOccurs="0"/>
                                                <element name="Direccion">
                                                    <complexType>
                                                        <sequence>
                                                            <element name="NombreMunicipio" type="ddii:TextMax30Type" minOccurs="0"/>
                                                            <element name="CodigoMunicipio" type="ddii:TextMax3Type"/>
                                                            <element name="CodigoProvincia" type="ddii:CodigoProvinciaType"/>
                                                            <element name="TipoVia" type="ddii:TextMax5Type"/>
                                                            <element name="NombreVia" type="ddii:TextMax50Type"/>
                                                            <element name="TipoNumeracion" type="ddii:TipoNumeracionType"/>
                                                            <element name="NumeroVivienda" type="ddii:TextMax5Type" minOccurs="0"/>
                                                            <element name="CalificadorNumero" type="ddii:TextMax3Type" minOccurs="0"/>
                                                            <element name="Bloque" type="ddii:TextMax3Type" minOccurs="0"/>
                                                            <element name="Portal" type="ddii:TextMax3Type" minOccurs="0"/>
                                                            <element name="Escalera" type="ddii:TextMax3Type" minOccurs="0"/>
                                                            <element name="PlantaPiso" type="ddii:TextMax3Type" minOccurs="0"/>
                                                            <element name="Puerta" type="ddii:TextMax3Type" minOccurs="0"/>
                                                            <element name="Complemento" type="ddii:TextMax40Type" minOccurs="0"/>
                                                        </sequence>
                                                    </complexType>
                                                </element>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                        <element name="DesgloseOperacion">
                            <complexType>
                                <annotation>
                                    <documentation xml:lang="es"> Datos del desglose operación </documentation>
                                </annotation>
                                <sequence>
                                    <element name="PeriodoCesion">
                                        <complexType>
                                            <sequence>
                                                <element name="FechaIntermediacion" type="ddii:fecha" minOccurs="0"/>
                                                <element name="FechaInicioCesion" type="ddii:fecha"/>
                                                <element name="NumeroDiasDisfrute" type="ddii:TextMax3Type"/>
                                            </sequence>
                                        </complexType>
                                    </element>
                                    <element name="Importe" type="ddii:ImporteSgn12.2Type"/>
                                    <element name="NumeroContrato" type="ddii:TextMax20Type" minOccurs="0"/>
                                    <element name="MedioPago" minOccurs="0">
                                        <complexType>
                                            <sequence>
                                                <element name="ClaveMedioPago" type="ddii:MedioPagoType" minOccurs="0"/>
                                                <element name="IDMedioPago" type="ddii:TextMax30Type" minOccurs="0"/>
                                            </sequence>
                                        </complexType>
                                    </element>
                                </sequence>
                            </complexType>
                        </element>
                    </sequence>
                </complexType>
            </element>
        </sequence>
    </complexType>
    <complexType name="DeclaradoType2">
        <annotation>
            <documentation xml:lang="es">Datos correspondientes al Declarado </documentation>
        </annotation>
        <sequence>
            <element name="IDRegistroDeclarado" type="ddii:TextMax20Type"/>
        </sequence>
    </complexType>
    <!-- Datos de presentacion en las respuestas -->
    <complexType name="DatosPresentacionType">
        <sequence>
            <element name="NIFPresentador" type="ddii:NIFType"/>
            <element name="TimestampPresentacion" type="ddii:Timestamp"/>
        </sequence>
    </complexType>
    <complexType name="DatosPresentacion2Type">
        <sequence>
            <element name="NIFPresentador" type="ddii:NIFType"/>
            <element name="TimestampPresentacion" type="ddii:Timestamp"/>
            <element name="CSV" type="string" minOccurs="0">
                <annotation>
                    <documentation xml:lang="es"> CSV asociado al registro </documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <!-- Datos de persona Física o jurídica : Denominación, representación, identificación (NIF) -->
    <complexType name="PersonaFisicaJuridicaESType">
        <annotation>
            <documentation xml:lang="es">Datos de una persona física o jurídica Española con un NIF asociado</documentation>
        </annotation>
        <sequence>
            <element name="NombreRazon" type="ddii:TextMax120Type"/>
            <element name="NIFRepresentante" type="ddii:NIFType" minOccurs="0"/>
            <element name="NIF" type="ddii:NIFType"/>
        </sequence>
    </complexType>
    <!-- Datos de persona Física o jurídica : Denominación, representación, identificación (NIF) -->
    <complexType name="PersonaFisicaJuridicaUnicaESType">
        <annotation>
            <documentation xml:lang="es">Datos de una persona física o jurídica Española con un NIF asociado</documentation>
        </annotation>
        <sequence>
            <element name="NombreRazon" type="ddii:TextMax120Type"/>
            <element name="NIF" type="ddii:NIFType"/>
        </sequence>
    </complexType>
    <!-- Datos de persona Física o jurídica : Denominación, representación, identificación (NIF/Otro) -->
    <complexType name="PersonaFisicaJuridicaType">
        <annotation>
            <documentation xml:lang="es">Datos de una persona física o jurídica Española o Extranjera</documentation>
        </annotation>
        <sequence>
            <element name="NombreRazon" type="ddii:TextMax120Type"/>
            <element name="NIFRepresentante" type="ddii:NIFType" minOccurs="0"/>
            <choice>
                <element name="NIF" type="ddii:NIFType"/>
                <element name="IDOtro" type="ddii:IDOtroType"/>
            </choice>
        </sequence>
    </complexType>
    <!-- Datos de persona Física o jurídica : Denominación, representación, identificación (NIF/Otro) -->
    <complexType name="IDOtroType">
        <annotation>
            <documentation xml:lang="es">Identificador de persona Física o jurídica distinto del NIF (Código pais, Tipo de Identificador, y hasta 15 caractéres) No se permite CodigoPais=ES e IDType=01-NIFContraparte para ese caso, debe utilizarse NIF en lugar de IDOtro. </documentation>
        </annotation>
        <sequence>
            <element name="CodigoPais" type="ddii:CountryType2" minOccurs="0"/>
            <element name="IDType" type="ddii:PersonaFisicaJuridicaIDTypeType"/>
            <element name="ID" type="ddii:TextMax20Type"/>
        </sequence>
    </complexType>
    <simpleType name="VersionModeloType">
        <restriction base="string">
            <enumeration value="1.0"/>
        </restriction>
    </simpleType>
    <!-- ==== Tipos básicos (Listas de claves y patrones) ============================================= -->
    <!-- Año en formato YYYY -->
    <simpleType name="YearType">
        <annotation>
            <documentation xml:lang="es"> Año en formato YYYY </documentation>
        </annotation>
        <restriction base="string">
            <length value="4"/>
            <pattern value="\d{4,4}"/>
        </restriction>
    </simpleType>
    <simpleType name="ModeloType">
        <annotation>
            <documentation xml:lang="es"> Modelo</documentation>
        </annotation>
        <restriction base="string">
            <length value="3"/>
            <pattern value="\d{3,3}"/>
        </restriction>
    </simpleType>
    <!-- Período dentro del ejercicio 1T/2T/3T/4T para trimestres -->
    <simpleType name="TipoPeriodoType">
        <annotation>
            <documentation xml:lang="es"> Período dentro del ejercicio en formato MM para meses, 1T/2T/3T/4T para trimestres o 0A para anual </documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="1T">
                <annotation>
                    <documentation xml:lang="es"> Primer trimestre </documentation>
                </annotation>
            </enumeration>
            <enumeration value="2T">
                <annotation>
                    <documentation xml:lang="es"> Segundo trimestre </documentation>
                </annotation>
            </enumeration>
            <enumeration value="3T">
                <annotation>
                    <documentation xml:lang="es"> Tercer trimestre </documentation>
                </annotation>
            </enumeration>
            <enumeration value="4T">
                <annotation>
                    <documentation xml:lang="es"> Cuarto trimestre </documentation>
                </annotation>
            </enumeration>
            <enumeration value="0A">
                <annotation>
                    <documentation xml:lang="es"> Anual </documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <simpleType name="ClaveType">
        <restriction base="string">
            <enumeration value="T">
                <annotation>
                    <documentation xml:lang="es">Titular </documentation>
                </annotation>
            </enumeration>
            <enumeration value="C">
                <annotation>
                    <documentation xml:lang="es">Cedente </documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <!-- Identificador único de facturas -->
    <simpleType name="TextoIDenvioType">
        <restriction base="string">
            <maxLength value="20"/>
        </restriction>
    </simpleType>
    <!-- Identificador único de facturas -->
    <simpleType name="TextoIDFacturaType">
        <restriction base="string">
            <minLength value="1"/>
            <maxLength value="60"/>
        </restriction>
    </simpleType>
    <!-- Tipo de 10 dígitos -->
    <simpleType name="Tipo10Type">
        <restriction base="string">
            <pattern value="\d{0,10}"/>
        </restriction>
    </simpleType>
    <!-- Importe de 15 dígitos (12+2) "." como separador decimal -->
    <simpleType name="ImporteSgn12.2Type">
        <restriction base="string">
            <pattern value="(\+|-)?\d{1,12}(\.\d{0,2})?"/>
        </restriction>
    </simpleType>
    <!-- Importe de 17 dígitos (14+2) "." como separador decimal -->
    <simpleType name="ImporteSgn14.2Type">
        <restriction base="string">
            <pattern value="(\+|-)?\d{1,14}(\.\d{0,2})?"/>
        </restriction>
    </simpleType>
    <!-- Tipo de 6 dígitos (3+2) "." como separador decimal -->
    <simpleType name="Tipo2.2Type">
        <restriction base="string">
            <pattern value="\d{1,3}(\.\d{0,2})?"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 500 caracteres -->
    <simpleType name="TextMax500Type">
        <restriction base="string">
            <maxLength value="500"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 150 caracteres -->
    <simpleType name="TextMax150Type">
        <restriction base="string">
            <maxLength value="150"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 120 caracteres -->
    <simpleType name="TextMax120Type">
        <restriction base="string">
            <maxLength value="120"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 100 caracteres -->
    <simpleType name="TextMax100Type">
        <restriction base="string">
            <maxLength value="100"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 65 caracteres -->
    <simpleType name="TextMax65Type">
        <restriction base="string">
            <maxLength value="65"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 65 caracteres -->
    <simpleType name="TextMax60Type">
        <restriction base="string">
            <maxLength value="60"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 50 caracteres -->
    <simpleType name="TextMax50Type">
        <restriction base="string">
            <maxLength value="50"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 40 caracteres -->
    <simpleType name="TextMax40Type">
        <restriction base="string">
            <maxLength value="40"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 34 caracteres -->
    <simpleType name="TextMax34Type">
        <restriction base="string">
            <maxLength value="34"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 30 caracteres -->
    <simpleType name="TextMax30Type">
        <restriction base="string">
            <maxLength value="30"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 20 caracteres -->
    <simpleType name="TextMax20Type">
        <restriction base="string">
            <maxLength value="20"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 12 caracteres -->
    <simpleType name="TextMax12Type">
        <restriction base="string">
            <maxLength value="12"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 15 caracteres -->
    <simpleType name="TextMax15Type">
        <restriction base="string">
            <maxLength value="15"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 15 caracteres -->
    <simpleType name="TextMax25Type">
        <restriction base="string">
            <maxLength value="25"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 5 caracteres -->
    <simpleType name="TextMax5Type">
        <restriction base="string">
            <maxLength value="5"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 3 caracteres -->
    <simpleType name="TextMax3Type">
        <restriction base="string">
            <maxLength value="3"/>
        </restriction>
    </simpleType>
    <!-- Cadena de 2 caracteres -->
    <simpleType name="TextMax2Type">
        <restriction base="string">
            <maxLength value="2"/>
        </restriction>
    </simpleType>
    <!-- Referencia Catastral -->
    <simpleType name="ReferenciaCatastralType">
        <restriction base="string">
            <maxLength value="25"/>
        </restriction>
    </simpleType>
    <!-- NIF: Secuencia de 9 dígitos o letras mayúsculas -->
    <simpleType name="NIFType">
        <annotation>
            <documentation xml:lang="es">NIF: Secuencia de 9 dígitos o letras mayúsculas</documentation>
        </annotation>
        <restriction base="string">
            <length value="9"/>
            <pattern value="(([a-z|A-Z]{1}\d{7}[a-z|A-Z]{1})|(\d{8}[a-z|A-Z]{1})|([a-z|A-Z]{1}\d{8}))"/>
        </restriction>
        <!--([A-Z]|\d){9}-->
    </simpleType>
    <simpleType name="fecha">
        <restriction base="string">
            <length value="10"/>
            <pattern value="\d{2,2}-\d{2,2}-\d{4,4}"/>
        </restriction>
    </simpleType>
    <simpleType name="Timestamp">
        <restriction base="string">
            <length value="19"/>
            <pattern value="\d{2,2}-\d{2,2}-\d{4,4} \d{2,2}:\d{2,2}:\d{2,2}"/>
        </restriction>
    </simpleType>
    <simpleType name="ClaveTipoComunicacionType">
        <restriction base="string">
            <enumeration value="A0">
                <annotation>
                    <documentation xml:lang="es"> A0 Alta (Se añade al libro un registro una nueva factura)</documentation>
                </annotation>
            </enumeration>
            <enumeration value="A1">
                <annotation>
                    <!-- TODO: Determinar los tipos de factura en los que se soporta este valor -->
                    <documentation xml:lang="es"> A1 Modificación (La información que se comunica sustituye a la existente relacionada)</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <!-- Códigos de situación de inmueble -->
    <simpleType name="SituacionInmuebleType">
        <restriction base="string">
            <enumeration value="01">
                <annotation>
                    <documentation xml:lang="es">Inmueble con referencia catastral situado en cualquier punto del territorio español, excepto País Vasco y Navarra</documentation>
                </annotation>
            </enumeration>
            <enumeration value="02">
                <annotation>
                    <documentation xml:lang="es">Inmueble con referencia catastral situado en la Comunidad Autónoma del País Vasco</documentation>
                </annotation>
            </enumeration>
            <enumeration value="03">
                <annotation>
                    <documentation xml:lang="es">Inmueble con referencia catastral situado en la Comunidad Foral de Navarra</documentation>
                </annotation>
            </enumeration>
            <enumeration value="04">
                <annotation>
                    <documentation xml:lang="es">Inmueble en cualquiera de las situaciones anteriores pero sin referencia catastral</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <!-- Permitira identificar aquellas facturas con importe de la factura superior a un umbral -->
    <simpleType name="MacrodatoType">
        <restriction base="string">
            <enumeration value="S"/>
            <enumeration value="N"/>
        </restriction>
    </simpleType>
    <!-- Tipo de medio de pago -->
    <simpleType name="MedioPagoType">
        <restriction base="string">
            <enumeration value="01">
                <annotation>
                    <documentation xml:lang="es">Transferencia</documentation>
                </annotation>
            </enumeration>
            <enumeration value="02">
                <annotation>
                    <documentation xml:lang="es">Tarjeta crédito/débito</documentation>
                </annotation>
            </enumeration>
            <enumeration value="03">
                <annotation>
                    <documentation xml:lang="es">Otros medios de pago</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <!-- Tipo de identificador fiscal de persona Física o jurídica -->
    <simpleType name="PersonaFisicaJuridicaIDTypeType">
        <restriction base="string">
            <enumeration value="02">
                <annotation>
                    <documentation xml:lang="es">NIF-IVA</documentation>
                </annotation>
            </enumeration>
            <enumeration value="03">
                <annotation>
                    <documentation xml:lang="es">Pasaporte</documentation>
                </annotation>
            </enumeration>
            <enumeration value="04">
                <annotation>
                    <documentation xml:lang="es">IDEnPaisResidencia</documentation>
                </annotation>
            </enumeration>
            <enumeration value="05">
                <annotation>
                    <documentation xml:lang="es">Certificado Residencia</documentation>
                </annotation>
            </enumeration>
            <enumeration value="06">
                <annotation>
                    <documentation xml:lang="es">Otro documento Probatorio</documentation>
                </annotation>
            </enumeration>
            <enumeration value="07">
                <annotation>
                    <documentation xml:lang="es">No Censado</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <!-- ISO 3166-1 alpha-2 codes -->
    <simpleType name="CountryType2">
        <restriction base="string">
            <enumeration value="AF"/>
            <enumeration value="AL"/>
            <enumeration value="DE"/>
            <enumeration value="AD"/>
            <enumeration value="AO"/>
            <enumeration value="AI"/>
            <enumeration value="AQ"/>
            <enumeration value="AG"/>
            <enumeration value="SA"/>
            <enumeration value="DZ"/>
            <enumeration value="AR"/>
            <enumeration value="AM"/>
            <enumeration value="AW"/>
            <enumeration value="AU"/>
            <enumeration value="AT"/>
            <enumeration value="AZ"/>
